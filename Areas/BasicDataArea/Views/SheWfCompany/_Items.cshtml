@model IPagedList<SheWfCompany>
@{
    Layout = "~/Views/Shared/_ItemsPartialLayout.cshtml";
}

@section Head
{
    <th>污染源编码</th>
    <th>污染源名称</th>
    <th>危险废物产生量 </th>
    <th>危险废物处置量 </th>
<th>危险废物处置类别 </th>
    <th class="align-center">操作</th>
}

@section Items
{

    @foreach (var item in Model)
    {
        <tr class="hover">
            <td>@item.MainCode</td>
            <td>@item.PollutionSourceName</td>
            <td>@item.WfChanSheng</td>
            <td>@item.WfChuZhi</td>
            <td>@item.WfSubClass</td>
            <td class="align-center">
                <a class="edit" href="javascript:showDataFormModalDialog('/BasicDataArea/SheWfCompany/FormForUpdate?Id=@item.Id','编辑')">编辑</a>|
                <a class="edit" href="javascript:confirmDelete('/BasicDataArea/SheWfCompany/Remove','Id=@item.Id',showSubmitResult)">删除</a>
            </td>
        </tr>
    }
}



