@using BasicUPMS.Areas.BasicDataArea.Models
@model IPagedList<CameraIndexModel>
@{
    Layout = "~/Views/Shared/_ItemsPartialLayout.cshtml";
}

@section Head
{
    <th>摄像头资源名称</th>
    <th>视频监控点名称</th>
    <th>联系人</th>
    <th>手机号</th>
    <th>安装地点</th>
    <th>网格名称</th>
    <th class="align-center">操作</th>
}

@section Items
{

    @foreach (var item in Model)
    {
        <tr class="hover">
            <td>@item.Name</td>
            <td>@item.CamName</td>
            <td>@item.Contacts</td>
            <td>@item.ContactsPhone</td>
            <td>@item.Adder</td>
            <td>@item.GridName</td>
            <td class="align-center">
                <a class="edit" href="javascript:showDataFormModalDialog('/BasicDataArea/Camera/FormForUpdateVarchar?Id=@item.Id&t=@(DateTime.Now.Millisecond)','编辑')">编辑</a>|
                <a class="edit" href="javascript:confirmDelete('/BasicDataArea/Camera/RemoveVarChar','Id=@item.Id',showSubmitResult)">删除</a>
            </td>
        </tr>
    }
}



