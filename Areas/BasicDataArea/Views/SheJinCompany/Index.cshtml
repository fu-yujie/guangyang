@model IPagedList<SheJinCompany>
@{
    Layout = "~/Views/Shared/_ListPageLayout.cshtml";
    ViewBag.Title = "涉重金属企业";
}
@section SearchCondition
{
    <tr>
        <th>污染源名称</th>
        <td>
            <input type="text" name="Name" class="txt"/>
        </td>
        <td>
            &nbsp;&nbsp;&nbsp;
            <input type="submit" class="btn-search tooltip" value="查询" />
        </td>
        <td>
            <a href="javascript:showDataFormModalDialog('/BasicDataArea/SheJinCompany/FormForAdd','添加涉重金属企业')">添加涉重金属企业</a> |
            <a _href="~/SheJinCompany/ExcelOutputs" onclick="excelOutput(this)" style="cursor: pointer;">Excel导出</a>
        </td>
    </tr>
}
<script type="text/javascript">
    //
    function excelOutput(obj) {
         var href = $(obj).attr('_href');
         window.location.href = href;
    }
</script>