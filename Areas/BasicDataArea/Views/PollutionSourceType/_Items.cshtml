
@model IPagedList<PollutionSourceType>
@{
    Layout = "~/Views/Shared/_ItemsPartialLayout.cshtml";
}

@section Head
{
    <th>编号</th>
    <th>名称</th>
    <th>说明</th>
    <th class="align-center">操作</th>
}

@section Items
{

    @foreach (var item in Model)
    {
        <tr class="hover">
            <td>@item.Code</td>
            <td>@item.Name</td>
            <td>@item.Remarks</td>
            <td class="align-center">
                <a class="edit" href="javascript:showDataFormModalDialog('/BasicDataArea/PollutionSourceType/FormForUpdateVarchar?Id=@item.Id&t=@(DateTime.Now.Millisecond)','编辑')">编辑</a> |
                <a class="edit" href="javascript:confirmDelete('/BasicDataArea/PollutionSourceType/RemoveVarChar','Id=@item.Id',showSubmitResult)">删除</a> 
                @*<a class="edit" href="javascript:showDataFormModalDialog('BasicDataArea/PollutionSourceType/ContentAdd?Id=@item.Id','类型内容管理')">类型内容管理</a>*@
            </td>
        </tr>
    }
}
