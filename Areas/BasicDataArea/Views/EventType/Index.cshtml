@model IPagedList<EventType>
@{
    Layout = "~/Views/Shared/_ListPageLayout.cshtml";
    ViewBag.Title = "案件污染类型管理";
}
@section SearchCondition
{
    <tr>
        <th>
            <label>编号</label>
        </th>
        <td>
            <input type="text" name="Code" class="txt" />
        </td>
        <th>
            <label>名称</label>
        </th>
        <td>
            <input type="text" name="Name" class="txt" />
        </td>
        <th>环境类型</th>
        <td class="vatop">
            @Html.DropDownList("HJType", (List<SelectListItem>)ViewBag.DropDownHJ)
        </td>
        <td>
            <input type="submit" class="btn-search tooltip" value="查询" />
        </td>
        <td><a href="javascript:showDataFormModalDialog('/BasicDataArea/EventType/FormForAdd','添加案件污染类型')">添加案件污染类型</a> </td>
    </tr>
}


